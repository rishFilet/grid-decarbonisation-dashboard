# Toronto Grid Data API Configuration
# Copy this file to .env and add your API keys

# Environment Canada Weather API (optional)
VITE_WEATHER_API_KEY=your_weather_api_key_here

# IESO API Key (if required)
VITE_IESO_API_KEY=your_ieso_api_key_here

# Ontario Energy Board API Key (if required)
VITE_OEB_API_KEY=your_oeb_api_key_here

# Natural Resources Canada API Key (if required)
VITE_NRCAN_API_KEY=your_nrcan_api_key_here

# API Configuration
VITE_API_TIMEOUT=10000
VITE_UPDATE_INTERVAL=5000
VITE_FALLBACK_TO_MOCK=true 